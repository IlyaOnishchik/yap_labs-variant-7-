using System;

namespace yap_lab_4._4
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Числа Армстронга");
            for (int i = 100; i < 1000; i++)
            {
                double sum = 0;
                for (int x = i; x > 0; x /= 10)
                {
                    sum += Math.Pow(x % 10, 3);
                }
                if (i == sum) Console.WriteLine($"{i}");
            }
        }
    }
}
